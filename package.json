    "ts-jest": "^27.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start:dev": "NODE_ENV=development tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect dist/main.js\"",
    "start:prod": "node --max-old-space-size=2560 dist/main.js",
    "m:g": "./node_modules/.bin/typeorm migration:generate -n $name -f dist/config/db.js",
    "m:run": "./node_modules/.bin/typeorm migration:run -f dist/config/db.js",
    "db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n=src/config/db.ts",
    "code:format": "prettier --write \"src/**/*.ts\"",
    "code:check": "prettier --check \"src/**/*.ts\"",
    "cli": "'./node_modules/.bin/ts-node' src/utils/cli.ts"
  }
}
